
custom_attributes:
       hostName: Activemq-Data-Prod
       appName: Activemq
       appEnv: prod
       appZone: DZ






systemctl enable newrelic-infra.service
systemctl start newrelic-infra.service
systemctl status newrelic-infra.service
systemctl stop newrelic-infra.service

cp -p /etc/newrelic-infra.yml /etc/newrelic-infra.yml-bckup
cat /etc/newrelic-infra.yml
